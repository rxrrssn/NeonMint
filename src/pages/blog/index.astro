---
import Layout from "../../layouts/Layout.astro";
import Hero from "../../components/blog/Hero.astro";
import Tags from "../../components/blog/Tags.astro";
import ListPosts from "../../components/blog/ListPosts.astro";
const pageTitle = "Portfolio | TJH ";
const description = "Welcome to the portfolio for Travis James Hornbuckle, a psychology student and Program Associate with a passion in web design and airplanes!";
const ogimage ={
    url: "/images/logo.svg",
    alt: "TJH logo."
};
---

<Layout  pageTitle={pageTitle} description={description} ogimage={ogimage}>
   
    <Hero />
    <Tags />
    <div class="px-8 max-sm:px-4">
        <ListPosts />
    </div>

</Layout>
